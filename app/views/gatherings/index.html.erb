<p id="notice"><%= notice %></p>

<h1>Listing Gatherings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th>Gather level</th>
      <th>X coords</th>
      <th>Y coords</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gatherings.each do |gathering| %>
      <tr>
        <td><%= gathering.name %></td>
        <td><%= gathering.company %></td>
        <td><%= gathering.gather_level %></td>
        <td><%= gathering.x_coords %></td>
        <td><%= gathering.y_coords %></td>
        <td><%= link_to 'Show', gathering %></td>
        <td><%= link_to 'Edit', edit_gathering_path(gathering) %></td>
        <td><%= link_to 'Destroy', gathering, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gathering', new_gathering_path %>
